:@template"ﬂ,@mixin full-frame {
	height: 768px;
	width: 1024px;
	position: absolute;
}

/* @group containers */

	.container {
		position: absolute;
		top: 0;
		right: 0;
		bottom: 0;
		left: 0;
		background: #000;
	}
	

	.frame {
		@include full-frame;
		overflow: hidden;
		top: 50%;
		left: 50%;
		margin-left: -512px;
		margin-top: -384px;
	}

	.scene {
		background-color: gray;
		text-align: center;
		color: blue;
		font-size: 650px;

		@include full-frame;
	}

/* @end */

/* @group general elements*/

	.btn-previous {
		display: block;
		background-color: green;
		border: solid 1px #000;
		width: 60px;
		height: 40px;
		font-size: 30px;
		text-align: center;
		position: absolute;
		left: 400px;
		z-index: 5;
	}

	.btn-next {
		display: block;
		background-color: green;
		border: solid 1px #000;
		width: 60px;
		height: 40px;
		font-size: 30px;
		text-align: center;
		position: absolute;
		left: 460px;
		z-index: 5;
	}

/* @end */

/* @group general animations/interactions */

	.hidden {
		opacity: 0;
		transition: visibility 1s linear , opacity 1s linear;		
		-moz-transition: visibility 1s linear , opacity 1s linear;
		-ms-transition: visibility 1s linear , opacity 1s linear;	
		-o-transition: visibility 1s linear , opacity 1s linear;
		-webkit-transition: visibility 1s linear , opacity 1s linear;		
		visibility: hidden;
	}

	.current {
		opacity: 1;
		visibility: visible;
	}

/* @group end */


/* @group 1 - title page */

	.start-screen-1 {
		z-index: 1;
	}

	.sky {
		background: url('../images/1_start-screen/sky.png') top left / cover no-repeat;
		height: 768px;
		position: absolute;
		width: 100%;
		z-index: 1;
	}

	.mountain {
		background: url('../images/1_start-screen/mountain.png') bottom left / cover no-repeat;
		bottom: 0;
		height: 300px;
		position: absolute;
		width: 100%;
		z-index: 1;
	}

	.title{
		background: url('../images/1_start-screen/text.png') top left / auto 400px no-repeat;
		height: 400px;
		left: 100px;
		position: absolute;
		top: 50px;
		width: 100%;
		z-index: 1;
	}

	.right-cloud{
		-webkit-animation: right 3s 5s ease-in-out;
		-moz-animation: right 3s 5s ease-in-out;
		-ms-animation: right 3s 5s ease-in-out;
		-o-animation: right 3s 5s ease-in-out;
		animation: right 3s 5s ease-in-out;
		-webkit-animation-fill-mode: forwards;
		-moz-animation-fill-mode: forwards;
		-ms-animation-fill-mode: forwards;
		-o-animation-fill-mode: forwards;
		animation-fill-mode: forwards;
		background: url('../images/1_start-screen/right_cloud.png') top left / auto 700px no-repeat;
		height: 768px;
		left: 200px;
		position: absolute;
		top: 0;
		width: 100%;
		z-index: 2;
	}


	.left-cloud{	
		-webkit-animation: left 3s 5s ease-in-out;
		-moz-animation: left 3s 5s ease-in-out;
		-ms-animation: left 3s 5s ease-in-out;
		-o-animation: left 3s 5s ease-in-out;
		animation: left 3s 5s ease-in-out;
		-webkit-animation-fill-mode: forwards;
		-moz-animation-fill-mode: forwards;
		-ms-animation-fill-mode: forwards;
		-o-animation-fill-mode: forwards;
		animation-fill-mode: forwards;
		background: url('../images/1_start-screen/left_cloud.png') top left / auto 800px no-repeat;
		height: 768px;
		left: -110px;
		position: absolute;
		top: -200px;
		width: 100%;
		z-index: 3;
	}

	.cloud-1 {
		-webkit-animation: cloud-1 40s 5s linear;
		-moz-animation: cloud-1 40s 5s linear;
		-ms-animation: cloud-1 40s 5s linear;
		-o-animation: cloud-1 40s 5s linear;
		animation: cloud-1 40s 5s linear;
		background: url('../images/1_start-screen/cloud1.png') top left / auto 200px no-repeat;
		height: 200px;
		left: 600px;
		position: absolute;
		top: 250px;
		width: 100%;
		z-index: 0;
	}

	.cloud-2 {
		-webkit-animation: cloud-2 40s 5s linear;
		-moz-animation: cloud-2 40s 5s linear;
		-ms-animation: cloud-2 40s 5s linear;
		-o-animation: cloud-2 40s 5s linear;
		animation: cloud-2 40s 5s linear;
		background: url('../images/1_start-screen/cloud2.png') top left / auto 200px no-repeat;
		height: 500px;
		left: 200px;
		position: absolute;
		top: 75px;
		width: 100%;
		z-index: 1;
	}


	/* @group animations */

		@-webkit-keyframes right {

			100% {
				-webkit-transform: translate3d(1000px, 0 , 0);
				-moz-transform: translate3d(1000px, 0 , 0);
				-o-transform: translate3d(1000px, 0 , 0);
				-ms-transform: translate3d(1000px, 0 , 0);
				transform: translate3d(1000px, 0 , 0);
			}

		}

		@-webkit-keyframes left {

			100% {
				-webkit-transform: translate3d(-1000px, 0 , 0);
				-moz-transform: translate3d(-1000px, 0 , 0);
				-o-transform: translate3d(-1000px, 0 , 0);
				-ms-transform: translate3d(-1000px, 0 , 0);
				transform: translate3d(-1000px, 0 , 0);
			}

		}
 
		@-webkit-keyframes cloud-1 {

			100% {
				-webkit-transform: translate3d(-1000px, 0 , 0);
				-moz-transform: translate3d(-1000px, 0 , 0);
				-o-transform: translate3d(-1000px, 0 , 0);
				-ms-transform: translate3d(-1000px, 0 , 0);
				transform: translate3d(-1000px, 0 , 0);
			}

		}

		@-webkit-keyframes cloud-2 {

			100% {
				-webkit-transform: translate3d(1000px, 0 , 0);
				-moz-transform: translate3d(1000px, 0 , 0);
				-o-transform: translate3d(1000px, 0 , 0);
				-ms-transform: translate3d(1000px, 0 , 0);
				transform: translate3d(1000px, 0 , 0);
			}

		}

	/* @end */

/* @end */

/* @group exposition-2 */

	.house-right {
		-webkit-transform-origin: 50% 95%;
		-moz-transform-origin: 50% 95%;
		-o-transform-origin: 50% 95%;
		-ms-transform-origin: 50% 95%;
		transform-origin: 50% 95%;
		width: 160px;
		height: 419px;
		position: absolute;
		bottom: 111px;
		right: 85px;
		background: url(../images/2_exposition/optimized/retina/house_right_2x.png) no-repeat bottom left;
		-webkit-background-size: 100% 100%;
		-moz-background-size: 100% 100%;
		background-size: 100% 100%;
	}


	.house-left {
		-webkit-transform-origin: 50% 100%;
		-moz-transform-origin: 50% 100%;
		-o-transform-origin: 50% 100%;
		-ms-transform-origin: 50% 100%;
		transform-origin: 50% 100%;
		width: 415.53px;
		height: 458.08px;
		position: absolute;
		bottom: 175px;
		right: 195px;
		background: url(../images/2_exposition/optimized/retina/house_left_2x.png) no-repeat bottom left;
		background-size: 100% 100%;
	}

	.cloud	{
		-webkit-transition: all 30s linear;
		   -moz-transition: all 30s linear;
		        transition: all 30s linear;		
		height: 299.46px;
		width: 395.87px;
		position: absolute;
		top: 20px;
		right: -100px;
		background: url(../images/2_exposition/optimized/retina/cloud_2x.png) no-repeat bottom left;
		background-size: 100% 100%;
	}

	.sign {
		-webkit-transition: -webkit-transform .5s linear 9s;
		-moz-transition: -webkit-transform .5s linear 9s;
		-o-transition: -webkit-transform .5s linear 9s;
		-ms-transition: -webkit-transform .5s linear 9s;
		transition: -webkit-transform .5s linear 9s;
		height: 141px;
		width: 173px;
		position: absolute;
		left: 406px;
		bottom: 177px;
		background: url(../images/2_exposition/optimized/retina/sign_2x.png) no-repeat bottom left;
		-webkit-background-size: 100% 100%;
		-moz-background-size: 100% 100%;
		background-size: 100% 100%;
		-webkit-transform: rotate(240deg);
		-moz-transform: rotate(-120deg);
		-o-transform: rotate(-120deg);
		-ms-transform: rotate(-120deg);
		transform: rotate(-120deg);
		-webkit-transform-origin: 90% 93%;
		-moz-transform-origin: 89.37% 86.02%;
		-o-transform-origin: 89.37% 86.02%;
		-ms-transform-origin: 89.37% 86.02%;
		transform-origin: 89.37% 86.02%;

	}

	.grass-top	{
		width: 100%;
		height: 485px;
		position: absolute;
		bottom: 0;
		background: url(../images/2_exposition/optimized/retina/grass_top_2x.png) no-repeat bottom left;
		background-size: 100% 100%;
	}

	.grass-bottom {
		width: 100%;
		height: 188px;
		position: absolute;
		bottom: 0;
		background: url(../images/2_exposition/optimized/retina/grass_bottom_2x.png) no-repeat  bottom left;
		background-size: 100% 100%;
	}

	.exposition-2 {
		background: #e3e4e5; /* Old browsers */
		background: -moz-radial-gradient(center, ellipse cover, #e3e4e5 0%, #155a93 100%); /* FF3.6+ */
		background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%,#e3e4e5), color-stop(100%,#155a93)); /* Chrome,Safari4+ */
		background: -webkit-radial-gradient(center, ellipse cover, #e3e4e5 0%,#155a93 100%); /* Chrome10+,Safari5.1+ */
		background: -o-radial-gradient(center, ellipse cover, #e3e4e5 0%,#155a93 100%); /* Opera 12+ */
		background: -ms-radial-gradient(center, ellipse cover, #e3e4e5 0%,#155a93 100%); /* IE10+ */
		background: radial-gradient(ellipse at center, #e3e4e5 0%,#155a93 100%); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e3e4e5', endColorstr='#155a93',GradientType=1 ); /* IE6-9 fallback on horizontal gradient */
	}

	/* @group animations */
		@-webkit-keyframes fall_left {
		  0% {
		    opacity: 1;
		    -webkit-transform: rotate3D(0,0,0,0deg);

		  }
		  60% {
		  	-webkit-transform: rotate3D(100, 25,0,150deg);
		  	opacity: 1;
		  }
		  100% {
		  	-webkit-transform: rotate3D(100, 25,0,150deg);

		  	opacity: 0;
		  }
		}
		@-moz-keyframes fall_left {
		  0% {
		    opacity: 1;
		    -moz-transform: rotate3D(0,0,0,0deg);

		  }
		  60% {
		  	-moz-transform: rotate3D(100,-25,0,150deg);
		  	opacity: 1;
		  }
		  100% {
		  	opacity: 0;
		  }
		}

		@-webkit-keyframes fall_right {
		  0% {
		    opacity: 1;
		    -webkit-transform: rotate3D(0,0,0,0deg);

		  }
		  60% {
		  	-webkit-transform: rotate3D(100,-25,0,150deg);
		  	opacity: 1;
		  }
		  100% {
		  	-webkit-transform: rotate3D(100,-25,0,150deg);

		  	opacity: 0;
		  }
		}
		@-moz-keyframes fall_left {
		  0% {
		    opacity: 1;
		    -moz-transform: rotate3D(0,0,0,0deg);

		  }
		  60% {
		  	-moz-transform: rotate3D(100,-25,0,150deg);
		  	opacity: 1;
		  }
		  100% {
		  	opacity: 0;
		  }
		}
		
		.house-right.go {
			-webkit-animation: fall_right 4s 5s ease-in-out forwards;
			-moz-animation: fall_right 1s 6s ease-in forwards;
		}

		.house-left.go {
			-webkit-animation: fall_left 4s 5s ease-in-out forwards;
			-moz-animation: fall_left 1s 6s ease-in forwards;
		}

		.sign.go {
			-webkit-transform: rotate( 360deg);
			-moz-transform: rotate( 360deg);
			-o-transform: rotate( 360deg);
			-ms-transform: rotate( 360deg);
			transform: rotate( 360deg);
		}

		.cloud.go {
			-webkit-transform: translate3d(-300% , 0 , 0);
			-moz-transform: translate3d(-300% , 0 , 0);
			-o-transform: translate3d(-300% , 0 , 0);
			-ms-transform: translate3d(-300% , 0 , 0);
			transform: translate3d(-300% , 0 , 0);
		}


	/* @end */

/* @end */




