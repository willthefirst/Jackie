	.container {
		position: absolute;
		top: 0;
		right: 0;
		bottom: 0;
		left: 0;
		background: #000;
	}
	

	.frame {
		height: 768px;
		overflow: hidden;
		position: absolute;
		top: 50%;
		left: 50%;
		margin-left: -512px;
		margin-top: -384px;
		width: 1024px;
	}

	.scene {
		background-color: gray;
		text-align: center;
		color: blue;
		font-size: 650px;


		height: 768px;
		position: absolute;
		width: 1024px;
	}

/* @end */

